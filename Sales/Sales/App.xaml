<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:infra="clr-namespace:Sales.Infrastructure"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Sales.App">
	<Application.Resources>
        <ResourceDictionary>
            <!--locator-->
            <infra:InstaceLocator x:Key="Locator"/>

            <!--Parameters-->
            <x:String x:Key="UrlAPI">http://salesapiservices.azurewebsites.net</x:String>
            <x:String x:Key="UrPrefix">/api</x:String>
            <x:String x:Key="UrlProductsController">/Products</x:String>
        </ResourceDictionary> 
	</Application.Resources>
</Application>